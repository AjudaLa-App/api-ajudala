{
  "name": "backend",
  "version": "0.0.1",
  "main": "index.js",
  "engines": {
    "node": "12.16.1"
  },
  "scripts": {
    "dev": "nodemon ./src/server.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
